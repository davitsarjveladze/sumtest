CREATE TABLE [IF NOT EXISTS] TABLE sums (
	`id` INT(11) NOT NULL AUTO_INCREMENT,
	`number_one` DECIMAL(10,0) NOT NULL DEFAULT '0',
	`number_two` DECIMAL(10,0) NOT NULL DEFAULT '0',
	`sum` DECIMAL(10,0) NOT NULL DEFAULT '0',
	`created_at` DATETIME NOT NULL DEFAULT datetime(),
	PRIMARY KEY (`id`) USING BTREE
)

